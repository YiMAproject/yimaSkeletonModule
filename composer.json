{
    "name": "rayamedia/yima-skeleton-module",
    "type": "yima-core-module",
    "description": "Yima Skeleton Application Based On ZF2.",
    "keywords": [
        "zf2 skeleton module",
        "yima skeleton",
        "raya media"
    ],
    "authors": [
        {
            "name": "Payam Naderi",
            "email": "naderi.payam@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "rayamedia/yima-plugin-installer": "dev-master"
    },
    "extra": {
        "yima-plugin-name": "ySkeletonModule"
    },
    "minimum-stability": "dev",
    "autoload" : {
        "psr-0" : {
            "ySkeletonModule\\" : "src/"
        },
        "classmap" : [
            "./Module.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.raya-media.com/"
        }
    ]
}
